<script type="text/javascript">
	$(document).ready(function(){
		$('.top.tabular.menu.tabone .item').tab({
			context: '#chart1'
		});

		$('.top.tabular.menu.tabtwo .item').tab({
			context: '#chart2'
		});

		display('day', 'english');

		// setup the form
		$('.wave-data.form').form({
			fields: {
				unitOfMeasure: 'checked',
				occurrence: 'checked'
			}
		});

		// watch for a change
		$('.ui.checkbox').checkbox('setting', 'onChange', function(){
			var time = $('.wave-data.form').form('get value', 'occurrence');
			var unit = $('.wave-data.form').form('get value', 'unitOfMeasure');

			display(time, unit);
		});

		function display(timeFrame, unitMessure){
			if (timeFrame === undefined || timeFrame === '' || timeFrame === null) timeFrame = 'day';
			if (unitMessure === undefined || unitMessure === '' || unitMessure === null) unitMessure = 'english';

			switch (timeFrame){
				case 'week': 
					if (unitMessure == 'metric') $('#water-temp').html('<img src="https://my.fit.edu/coastal/WTmetric1week.jpg" height="600" width="800" alt="graph" />');
					else $('#water-temp').html('<img src="https://my.fit.edu/coastal/WT1week.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#water-level').html('<img src="https://my.fit.edu/coastal/WLmetric1week.jpg" height="600" width="800" alt="graph" />');
					else $('#water-level').html('<img src="https://my.fit.edu/coastal/WL1week.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#wave-height').html('<img src="https://my.fit.edu/coastal/FIT_SISP_RDI/WHmetric1week.jpg" height="600" width="800" alt="graph" />');
					else $('#wave-height').html('<img src="https://my.fit.edu/coastal/FIT_SISP_RDI/WH1week.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#air-temp').html('<img src="https://my.fit.edu/coastal/ATmetric1week.jpg" height="600" width="800" alt="graph" />');
					else $('#air-temp').html('<img src="https://my.fit.edu/coastal/AT1week.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#wind-speed').html('<img src="https://my.fit.edu/coastal/WSmetric1week.jpg" height="600" width="800" alt="graph" />');
					else $('#wind-speed').html('<img src="https://my.fit.edu/coastal/WS1week.jpg" height="600" width="800" alt="graph" />');

					$('#sebastian-inlet-wind-dir').html('<p>Currently unavailable.<\/p>');
					$('#ndbc-buoy-wind-dir').html('<p>Currently unavailable.<\/p>');
					$('#wave-direction').html('<p>Currently unavailable.<\/p>');
				break;

				case 'month': 
					if (unitMessure == 'metric') $('#water-temp').html('<img src="https://my.fit.edu/coastal/WTmetric4week.jpg" height="600" width="800" alt="graph" />');
					else $('#water-temp').html('<img src="https://my.fit.edu/coastal/WT4week.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#water-level').html('<img src="https://my.fit.edu/coastal/WLmetric4week.jpg" height="600" width="800" alt="graph" />');
					else $('#water-level').html('<img src="https://my.fit.edu/coastal/WL4week.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#wave-height').html('<img src="https://my.fit.edu/coastal/FIT_SISP_RDI/WHmetric4week.jpg" height="600" width="800" alt="graph" />');
					else $('#wave-height').html('<img src="https://my.fit.edu/coastal/FIT_SISP_RDI/WH4week.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#air-temp').html('<img src="https://my.fit.edu/coastal/ATmetric4week.jpg" height="600" width="800" alt="graph" />');
					else $('#air-temp').html('<img src="https://my.fit.edu/coastal/AT4week.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#wind-speed').html('<img src="https://my.fit.edu/coastal/WSmetric4week.jpg" height="600" width="800" alt="graph" />');
					else $('#wind-speed').html('<img src="https://my.fit.edu/coastal/WS4week.jpg" height="600" width="800" alt="graph" />');

					$('#sebastian-inlet-wind-dir').html('<p>Currently unavailable.<\/p>');
					$('#ndbc-buoy-wind-dir').html('<p>Currently unavailable.<\/p>');
					$('#wave-direction').html('<p>Currently unavailable.<\/p>');
				break;

				case 'day':
				default:
					if (unitMessure == 'metric'){ 
						$('#water-temp').html('<img src="https://my.fit.edu/coastal/WTmetric24hour.jpg" height="600" width="800" alt="graph" />');
						console.log($('#water-temp'));
					}
					else $('#water-temp').html('<img src="https://my.fit.edu/coastal/WT24hour.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#water-level').html('<img src="https://my.fit.edu/coastal/WLmetric24hour.jpg" height="600" width="800" alt="graph" />');
					else $('#water-level').html('<img src="https://my.fit.edu/coastal/WL24hour.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#wave-height').html('<img src="https://my.fit.edu/coastal/FIT_SISP_RDI/WHmetric24hour.jpg" height="600" width="800" alt="graph" />');
					else $('#wave-height').html('<img src="https://my.fit.edu/coastal/FIT_SISP_RDI/WH24hour.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#air-temp').html('<img src="https://my.fit.edu/coastal/ATmetric24hour.jpg" height="600" width="800" alt="graph" />');
					else $('#air-temp').html('<img src="https://my.fit.edu/coastal/AT24hour.jpg" height="600" width="800" alt="graph" />');

					if (unitMessure == 'metric') $('#wind-speed').html('<img src="https://my.fit.edu/coastal/WSmetric24hour.jpg" height="600" width="800" alt="graph" />');
					else $('#wind-speed').html('<img src="https://my.fit.edu/coastal/WS24hour.jpg" height="600" width="800" alt="graph" />');

					$('#sebastian-inlet-wind-dir').html('<img src="https://my.fit.edu/coastal/WindRose_SISPNJ.jpg" height="600" width="800" alt="graph" />');
					$('#ndbc-buoy-wind-dir').html('<img src="https://my.fit.edu/coastal/WindRose_TRDF1.jpg" height="600" width="800" alt="graph" />');
					$('#wave-direction').html('<img src="https://my.fit.edu/coastal/FIT_SISP_RDI/WaveRose.jpg" height="600" width="800" alt="graph" />');
				break;
			};
		}
	});
</script>

<p style="text-align: center;">These raw data have not been quality controlled and should be used with caution. They are released for limited public use within the academic and scientific community.</p>
</div>
